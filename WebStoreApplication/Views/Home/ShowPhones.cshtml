@model PhoneViewModel

    <div>
        <table>
            @foreach (Phone phone in Model.Phones)
            {
                <tr>
                    <td>
                        @phone.Name
                    </td>
                    <td>
                        <img src="@phone.ImageUrl" alt="" width="100" height="100" />
                    </td>
                    <td>
                        @phone.Price.ToString("c")
                    </td>
                </tr>
            }
        </table>
    </div>
